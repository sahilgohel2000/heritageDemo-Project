<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/dialogGetThere"
    android:paddingTop="15.00dp"
    android:background="@color/white"
    tools:context=".Activity.HeritageSiteDetailActivity">

<TextView
    android:id="@+id/titleText"
    android:text="Select Would you like to Go"
    android:textAlignment="center"
    android:textStyle="bold"
    android:textSize="20dp"
    android:textColor="@color/black"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintBottom_toTopOf="@id/tv_driving"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintBottom_toTopOf="@id/tv_driving"/>

    <TextView
        android:id="@+id/tv_driving"
        android:text="Driving"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/titleText"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_driving"/>

    <TextView
        android:id="@+id/tv_public_transport"
        android:text="Public Transport"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_driving"
        app:layout_constraintBottom_toTopOf="@id/tv_public_transport"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_public_transport"/>

    <TextView
        android:id="@+id/tv_myByk"
        android:text="MyByk"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5dp"
        app:layout_constraintBottom_toTopOf="@id/tv_Uber"
        app:layout_constraintTop_toBottomOf="@id/tv_public_transport"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_myByk"/>

    <TextView
        android:id="@+id/tv_Uber"
        android:text="Uber"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5dp"
        app:layout_constraintBottom_toTopOf="@id/tv_Ola"
        app:layout_constraintTop_toBottomOf="@id/tv_myByk"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintBottom_toTopOf="@id/tv_Ola"
        app:layout_constraintTop_toBottomOf="@id/tv_Uber"/>

    <TextView
        android:id="@+id/tv_Ola"
        android:text="Ola"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/tv_walking"
        app:layout_constraintTop_toBottomOf="@id/tv_Uber"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_Ola"/>

    <TextView
        android:id="@+id/tv_walking"
        android:text="Walking"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_Ola"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/colorGray"
        app:layout_constraintTop_toBottomOf="@id/tv_walking"/>
    <TextView
        android:id="@+id/tv_Cancel"
        android:text="Cancel"
        android:textSize="18dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="@color/colorOrange"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/_5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_walking"/>

</androidx.constraintlayout.widget.ConstraintLayout>